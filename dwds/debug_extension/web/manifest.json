{
    "name": "Dart Debug Extension",
    "version": "1.21",
    "minimum_chrome_version": "10.0",
    "devtools_page": "devtools.html",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "dart.png"
    },
    "permissions": [
        "tabs",
        "debugger"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}
